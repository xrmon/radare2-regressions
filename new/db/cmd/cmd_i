            ;-- __preinit2:
            0x08049ed8      .dword 0x0804849f ; entry2.preinit ; sym.preinit2

            ;-- section_end..preinit_array:
            ;-- section..init_array:
            ;-- __frame_dummy_init_array_entry:
            ;-- __init_array_start:
            0x08049edc      .dword 0x08048480 ; entry3.init ; sym.frame_dummy ; [19] -rw- section size 20 named .init_array
            0x08049ee0      .dword 0x080484b8 ; entry4.init ; sym.constructor
            ;-- __init:
            0x08049ee4      .dword 0x080484d1 ; entry5.init ; sym.init
            ;-- __init2:
