            ;-- section..fini_array:
            ;-- __do_global_dtors_aux_fini_array_entry:
            ;-- __init_array_end:
            0x08049ef0      .dword 0x08048450 ; entry8.fini ; sym.__do_global_dtors_aux ; [20] -rw- section size 20 named .fini_array
            0x08049ef4      .dword 0x0804859c ; entry9.fini ; sym.destructor2
